{
    "model": {
        "input_size": 17,
        "output_size": 1,
        "seed": 1036272856,
        "layers": [
            {
                "num_neurons": 2,
                "activation_function": "relu",
                "initialization": "xavier_initializer"
            },
            {
                "num_neurons": 1,
                "activation_function": "sigmoid",
                "initialization": "he_initializer"
            }
        ]
    },
    "training": {
        "loss_function": "mean_squared_error",
        "epochs": 100,
        "batch_size": 8,
        "print_every": 100,
        "print_loss": true,
        "learning_rate": 0.1,
        "regularization_lambda": 0,
        "momentum": 0.7,
        "learning_rate_schedule": {
            "approach": "cosine_annealing",
            "params": {
                "min_lr": 0.0001,
                "decay_rate": 0.8,
                "step_size": 20,
                "warmup_epochs": 5
            }
        },
        "early_stopping": {
            "enabled": true,
            "patience": 10,
            "min_delta": 0.001
        }
    },
    "preprocessing": {
        "name": "one_hot_encode"
    }
}