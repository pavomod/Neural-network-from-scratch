{
    "model": {
        "input_size": 6,
        "output_size": 1,
        "seed": 42,
        "layers": [
            {
                "num_neurons": 4,
                "activation_function": "relu",
                "initialization": "he_initializer"
            },
            {
                "num_neurons": 1,
                "activation_function": "sigmoid",
                "initialization": "xavier_initializer"
            }
        ]
    },
    "training": {
        "loss_function": "mean_squared_error",
        "epochs": 300,
        "batch_size": 16,
        "learning_rate": 0.01,
        "regularization_lambda": 0.001,
        "momentum": 0.91,
        "learning_rate_schedule": {
            "approach": "cosine_annealing",
            "params": {
                "min_lr": 0.0001,
                "decay_rate": 0.8,
                "step_size": 20,
                "warmup_epochs": 5
            }
        }
    }
}